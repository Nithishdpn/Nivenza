{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/Contact",
      "destination": "/Contact.html"
    },
    {
      "source": "/Company", 
      "destination": "/Company.html"
    },
    {
      "source": "/work",
      "destination": "/work.html"
    },
    {
      "source": "/getstart",
      "destination": "/getstart.html"
    },
    {
      "source": "/case-study",
      "destination": "/case-study.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/Contact.html",
      "destination": "/Contact",
      "permanent": true
    },
    {
      "source": "/Company.html",
      "destination": "/Company", 
      "permanent": true
    },
    {
      "source": "/work.html",
      "destination": "/work",
      "permanent": true
    },
    {
      "source": "/getstart.html",
      "destination": "/getstart",
      "permanent": true
    },
    {
      "source": "/case-study.html",
      "destination": "/case-study",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
